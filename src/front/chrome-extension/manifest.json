{
  "manifest_version": 2,
  "version": "1.3.3",
  "name": "MultiCurrencyWallet",
  "short_name": "MultiCurrencyWallet",
  "description": "Hot wallet with p2p exchange",
  "homepage_url": "https://swaponline.io/",
  "icons": {
    "16": "./icons/icon-16.png",
    "32": "./icons/icon-32.png",
    "192": "./icons/icon-196.png"
  },
  "options_page": "application/index.html",
  "browser_action": {
    "default_icon": "./icons/icon-32.png",
    "default_title": "MultiCurrencyWallet"
  },
  "background": {
    "page": "application/index.html",
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://unpkg.com/ 'sha256-TR7MLOIXkVyhGy1opDnK9AYBn91yweeiK+ZugIb3/8o='; object-src 'self'"
}
